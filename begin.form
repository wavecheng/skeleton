<table border="1">
	<tr>
		<td>请假类型：</td>
		<td>
			<select id="leaveType" name="fp_leaveType">
				<option>公休</option>
				<option>病假</option>
				<option>调休</option>
				<option>事假</option>
				<option>婚假</option>
			</select>
		</td>
	</tr>
	<tr>
		<td>开始时间：</td>
		<td><input type="text" id="startTime" name="startDate" class="datetime required" /></td>
	</tr>
	<tr>
		<td>请假天数：</td>
		<td><input type="text" id="endTime" name="numOfDays" class="datetime required" /></td>
	</tr>
	<tr>
		<td>请假原因：</td>
		<td>
			<textarea id="reason" name="reason"></textarea>
		</td>
	</tr>
</table>